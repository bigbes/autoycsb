daemonize yes
pidfile /var/run/redis/redis_{{ redis.port }}.pid
logfile /var/log/redis/redis_{{ redis.port }}.log
port {{ redis.port }}
timeout 0
loglevel warning
databases 1

appendonly no

slowlog-log-slower-than 10000
slowlog-max-len 128

list-max-ziplist-entries 512
list-max-ziplist-value 64

set-max-intset-entries 512

zset-max-ziplist-entries 128
zset-max-ziplist-value 64

activerehashing yes
